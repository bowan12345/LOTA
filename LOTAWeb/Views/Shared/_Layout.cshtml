<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LOTA Admin Portal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LOTAWeb.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css" />
    <!-- Bootstrap Datepicker CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script>
    @await RenderSectionAsync("Styles", required: false)
</head>
<body>
    <!-- Top Header -->
    <header class="bg-success text-white shadow-sm d-flex align-items-center" style="min-height: 80px;">
        <nav class="navbar navbar-expand-lg navbar-dark w-100">
            <div class="container-fluid d-flex align-items-center">
                 <!-- Logo and Brand -->
                 <div class="d-flex align-items-center">
                     <a class="navbar-brand fw-bold d-flex align-items-center" asp-area="" asp-controller="Home" asp-action="Index">
                         <img src="~/image/logo.jpg" alt="LOTA Logo" class="me-3" style="width: 40px; height: 40px; object-fit: cover; border-radius: 50%;">
                        LOTA Admin Portal
                     </a>
                 </div>

                <!-- Right side content -->
                <div class="d-flex align-items-center">
                    <span class="me-3">Welcome, Administrator</span>
                    <button class="btn btn-outline-light btn-sm rounded-pill">
                        <i class="fas fa-sign-out-alt me-1"></i>
                        Logout
                    </button>
                </div>
            </div>
        </nav>
    </header>

    <div class="d-flex">
         <!-- Sidebar -->
         <nav id="sidebar" class="bg-white shadow-sm" style="width: 280px; min-height: calc(100vh - 80px);">
            <div class="p-3">
                <h5 class="fw-bold text-dark mb-3">Navigation</h5>
                <hr class="my-3">
                
                <ul class="nav flex-column">
                    <!-- Dashboard Section -->
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" asp-area="Admin" asp-controller="Home" asp-action="Index">
                            <i class="fas fa-home me-3"></i>
                            Dashboard
                        </a>
                    </li>
                    
                    <!-- Subtle Divider -->
                    <div class="nav-divider"></div>
                    
                    <!-- Manage Programs Dropdown -->
                    <li class="nav-item mb-2">
                        <button class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded dropdown-toggle w-100 text-start border-0 bg-transparent nav-main-item"
                                data-bs-toggle="collapse"
                                data-bs-target="#manageProgramsCollapse"
                                type="button"
                                aria-expanded="false"
                                aria-controls="manageProgramsCollapse"
                                id="manageProgramsToggle">
                            <i class="fas fa-cogs me-3"></i>
                            Manage Programs
                        </button>
                        <div class="collapse" id="manageProgramsCollapse">
                            <ul class="nav flex-column ms-3 mt-1">
                                <li class="nav-item mb-1">
                                    <a class="nav-link text-dark d-flex align-items-center py-1 px-3 rounded nav-sub-item" asp-area="Admin" asp-controller="Trimester" asp-action="Index">
                                        <i class="fas fa-calendar-week me-3"></i>
                                        Manage Trimesters
                                    </a>
                                </li>
                                <li class="nav-item mb-1">
                                    <a class="nav-link text-dark d-flex align-items-center py-1 px-3 rounded nav-sub-item" asp-area="Admin" asp-controller="Qualification" asp-action="Index">
                                        <i class="fas fa-certificate me-3"></i>
                                        Manage Qualifications
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </li>

                    <!-- Subtle Divider -->
                    <div class="nav-divider"></div>

                    <!-- People Management Section -->
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" asp-area="Admin" asp-controller="Tutor" asp-action="Index">
                            <i class="fas fa-user-tie me-3"></i>
                            Manage Tutors
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" asp-area="Admin" asp-controller="Student" asp-action="Index">
                            <i class="fas fa-graduation-cap me-3"></i>
                            Manage Students
                        </a>
                    </li>
                    
                    <!-- Subtle Divider -->
                    <div class="nav-divider"></div>
                    
                    <!-- Manage Courses Dropdown -->
                    <li class="nav-item mb-2">
                        <button class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded dropdown-toggle w-100 text-start border-0 bg-transparent nav-main-item" 
                                data-bs-toggle="collapse" 
                                data-bs-target="#manageCoursesCollapse" 
                                type="button"
                                aria-expanded="false" 
                                aria-controls="manageCoursesCollapse"
                                id="manageCoursesToggle">
                            <i class="fas fa-book me-3"></i>
                            Manage Courses
                        </button>
                        <div class="collapse" id="manageCoursesCollapse">
                            <ul class="nav flex-column ms-3 mt-1">
                                <li class="nav-item mb-1">
                                    <a class="nav-link text-dark d-flex align-items-center py-1 px-3 rounded nav-sub-item" asp-area="Admin" asp-controller="Course" asp-action="Index">
                                        <i class="fas fa-list me-3"></i>
                                        Course Catalog
                                    </a>
                                </li>
                                <li class="nav-item mb-1">
                                    <a class="nav-link text-dark d-flex align-items-center py-1 px-3 rounded nav-sub-item" asp-area="Admin" asp-controller="Course" asp-action="CourseOffering">
                                        <i class="fas fa-chalkboard-teacher me-3"></i>
                                        Course Offerings
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </li>
                    
                    <!-- Subtle Divider -->
                    <div class="nav-divider"></div>
                    
                    <!-- Assessment & Results Section -->
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" asp-area="Admin" asp-controller="Assessment" asp-action="Index">
                            <i class="fas fa-file-alt me-3"></i>
                            Manage Assessments
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" href="#">
                            <i class="fas fa-list me-3"></i>
                            LO Scores
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a class="nav-link text-dark d-flex align-items-center py-2 px-3 rounded nav-main-item" href="#">
                            <i class="fas fa-chart-line me-3"></i>
                            LO Results
                        </a>
                    </li>
                </ul>
            </div>
        </nav>

        <!-- Main Content -->
        <div class="flex-grow-1">
            <main role="main" class="p-4">
                @RenderBody()
            </main>
        </div>
    </div>

         <!-- Mobile Sidebar Toggle -->
     <button id="sidebarToggle" class="btn btn-success d-lg-none position-fixed" style="top: 90px; left: 10px; z-index: 1030;">
        <i class="fas fa-bars"></i>
    </button>

    <!-- Mobile Sidebar Overlay -->
    <div id="sidebarOverlay" class="d-lg-none position-fixed w-100 h-100 bg-dark bg-opacity-50" style="top: 0; left: 0; z-index: 1020; display: none;"></div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script>
    <script src="~/js/sweetalert-utils.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    

    
    <script>
        // Sidebar toggle functionality
        document.addEventListener('DOMContentLoaded', function() {
            const sidebarToggle = document.getElementById('sidebarToggle');
            const sidebar = document.getElementById('sidebar');
            const sidebarOverlay = document.getElementById('sidebarOverlay');
            
            sidebarToggle.addEventListener('click', function() {
                sidebar.classList.toggle('show');
                sidebarOverlay.style.display = sidebar.classList.contains('show') ? 'block' : 'none';
            });
            
            sidebarOverlay.addEventListener('click', function() {
                sidebar.classList.remove('show');
                sidebarOverlay.style.display = 'none';
            });
            
            // Close sidebar on window resize
            window.addEventListener('resize', function() {
                if (window.innerWidth >= 992) {
                    sidebar.classList.remove('show');
                    sidebarOverlay.style.display = 'none';
                }
            });
                 });
         
                   // Set active navigation based on current page
          function setActiveNavigation() {
              const currentPath = window.location.pathname.toLowerCase();
              const allNavLinks = document.querySelectorAll('#sidebar .nav-link, #sidebar button.nav-link');
              const manageProgramsCollapse = document.getElementById('manageProgramsCollapse');
              const manageProgramsToggle = document.getElementById('manageProgramsToggle');
              const manageCoursesCollapse = document.getElementById('manageCoursesCollapse');
              const manageCoursesToggle = document.getElementById('manageCoursesToggle');
              
              let hasActiveProgramsSubMenu = false;
              let hasActiveCoursesSubMenu = false;
              
              // Reset all navigation items first
              allNavLinks.forEach(item => {
                  item.classList.remove('active', 'bg-primary', 'text-white');
                  item.classList.add('text-dark');
              });
              
              // Check all navigation links for matches
              allNavLinks.forEach((link) => {
                  // Skip the dropdown toggle button for href checking
                  if (link.id === 'manageProgramsToggle' || link.id === 'manageCoursesToggle') {
                      return;
                  }
                  
                  // Get the href and normalize it
                  const href = link.getAttribute('href');
                  
                  if (href && href !== '#') {
                      let normalizedHref = href.toLowerCase();
                      
                      // Create more flexible matching logic
                      let isMatch = false;
                      
                      // Match using contains logic with more flexible patterns
                      if ((normalizedHref.includes('/course') && currentPath.includes('/course')) && 
                          !normalizedHref.includes('courseoffering') && !currentPath.includes('courseoffering')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/course/courseoffering') && currentPath.includes('/course/courseoffering')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/trimester') && currentPath.includes('/trimester')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/qualification') && currentPath.includes('/qualification')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/assessment') && currentPath.includes('/assessment')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/student') && currentPath.includes('/student')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/tutor') && currentPath.includes('/tutor')) {
                          isMatch = true;
                      } else if (normalizedHref.includes('/home') && currentPath.includes('/home')) {
                          isMatch = true;
                      }
                      
                                             if (isMatch) {
                           // Highlight the matched navigation item
                           link.classList.add('active', 'bg-primary', 'text-white');
                           link.classList.remove('text-dark');
                           
                           // Check if this active link is inside the Manage Programs submenu
                           if (link.closest('#manageProgramsCollapse')) {
                               hasActiveProgramsSubMenu = true;
                           }
                           
                           // Check if this active link is inside the Manage Courses submenu
                           if (link.closest('#manageCoursesCollapse')) {
                               hasActiveCoursesSubMenu = true;
                           }
                           
                           // Add active class to parent dropdown if submenu item is active
                           if (link.closest('#manageProgramsCollapse') && manageProgramsToggle) {
                               manageProgramsToggle.classList.add('active');
                           }
                           if (link.closest('#manageCoursesCollapse') && manageCoursesToggle) {
                               manageCoursesToggle.classList.add('active');
                           }
                       }
                  }
              });
              
              // Handle Manage Programs dropdown
              if (hasActiveProgramsSubMenu && manageProgramsToggle && manageProgramsCollapse) {
                  // Expand the submenu
                  manageProgramsCollapse.classList.add('show');
                  manageProgramsToggle.setAttribute('aria-expanded', 'true');
              } else if (manageProgramsToggle && manageProgramsCollapse) {
                  // Ensure submenu is collapsed if no submenu items are active
                  manageProgramsCollapse.classList.remove('show');
                  manageProgramsToggle.setAttribute('aria-expanded', 'false');
              }
              
              // Handle Manage Courses dropdown
              if (hasActiveCoursesSubMenu && manageCoursesToggle && manageCoursesCollapse) {
                  // Expand the submenu
                  manageCoursesCollapse.classList.add('show');
                  manageCoursesToggle.setAttribute('aria-expanded', 'true');
              } else if (manageCoursesToggle && manageCoursesCollapse) {
                  // Ensure submenu is collapsed if no submenu items are active
                  manageCoursesCollapse.classList.remove('show');
                  manageCoursesToggle.setAttribute('aria-expanded', 'false');
              }
          }
         
         // Call the function when page loads
         setActiveNavigation();
     </script>

    <style>
        /* Custom styles for the layout */
        body {
            background-color: #f8f9fa;
        }
        
        /* Sidebar styles */
        #sidebar {
            transition: transform 0.3s ease-in-out;
        }
        
        /* Main navigation items */
        #sidebar .nav-main-item {
            transition: all 0.2s ease-in-out;
            border: 1px solid transparent;
            font-weight: 500;
            position: relative;
            background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
        }
        
        #sidebar .nav-main-item:hover {
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%) !important;
            border-color: #dee2e6;
            transform: translateX(3px);
            box-shadow: 0 2px 6px rgba(0,0,0,0.08);
        }
        
        #sidebar .nav-main-item.active {
            background: linear-gradient(135deg, #0d6efd 0%, #0b5ed7 100%) !important;
            color: white !important;
            border-color: #0d6efd;
            box-shadow: 0 3px 8px rgba(13, 110, 253, 0.3);
        }
        
        /* Sub-navigation items */
        #sidebar .nav-sub-item {
            transition: all 0.2s ease-in-out;
            font-size: 0.9rem;
            font-weight: 400;
            background: transparent;
        }
        
        #sidebar .nav-sub-item:hover {
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%) !important;
            transform: translateX(3px);
        }
        
        #sidebar .nav-sub-item.active {
            background: linear-gradient(135deg, #0d6efd 0%, #0b5ed7 100%) !important;
            color: white !important;
        }
        
        /* Subtle divider styling - integrated with the design */
        .nav-divider {
            height: 1px;
            background: linear-gradient(90deg, transparent 0%, #dee2e6 20%, #dee2e6 80%, transparent 100%);
            margin: 0.5rem 0;
            opacity: 0.4;
        }
        
        /* Navigation item spacing - keep compact */
        #sidebar .nav-item {
            margin-bottom: 0.5rem;
        }
        
        /* Dropdown toggle styling */
        #sidebar .dropdown-toggle {
            position: relative;
            overflow: hidden;
        }
        
        #sidebar .dropdown-toggle::before {
            content: '';
            position: absolute;
            left: 0;
            top: 0;
            height: 100%;
            width: 3px;
            background: linear-gradient(180deg, #6c757d 0%, #495057 100%);
            transition: all 0.2s ease-in-out;
        }
        
        #sidebar .dropdown-toggle:hover::before {
            background: linear-gradient(180deg, #0d6efd 0%, #0b5ed7 100%);
            width: 4px;
        }
        
        #sidebar .dropdown-toggle.active::before {
            background: linear-gradient(180deg, #ffffff 0%, #f8f9fa 100%);
            width: 4px;
        }
        
                 /* Manage Programs dropdown styles */
         #manageProgramsCollapse {
             transition: all 0.3s ease-in-out;
         }
         
         #manageProgramsCollapse .nav-link {
             font-size: 0.9rem;
             padding-left: 2.5rem;
             border-left: 2px solid #e9ecef;
             margin-left: 0.5rem;
         }
         
         #manageProgramsCollapse .nav-link:hover {
             background-color: #f8f9fa !important;
             border-left-color: #0d6efd;
         }
         
         /* Manage Courses dropdown styles */
         #manageCoursesCollapse {
             transition: all 0.3s ease-in-out;
         }
         
         #manageCoursesCollapse .nav-link {
             font-size: 0.9rem;
             padding-left: 2.5rem;
             border-left: 2px solid #e9ecef;
             margin-left: 0.5rem;
         }
         
         #manageCoursesCollapse .nav-link:hover {
             background-color: #f8f9fa !important;
             border-left-color: #0d6efd;
         }
        
        .dropdown-toggle::after {
            margin-left: auto;
            transition: transform 0.2s ease-in-out;
        }
        
        .dropdown-toggle[aria-expanded="true"]::after {
            transform: rotate(180deg);
        }
        
        /* Style for active parent menu when submenu is active */
        #manageProgramsToggle.active {
            background-color: #0d6efd !important;
            color: white !important;
        }
        
                 /* Ensure submenu active items are properly styled */
         #manageProgramsCollapse .nav-link.active {
             background-color: #0d6efd !important;
             color: white !important;
             border-left-color: #ffffff;
         }
         
         #manageCoursesCollapse .nav-link.active {
             background-color: #0d6efd !important;
             color: white !important;
             border-left-color: #ffffff;
         }
        
        /* Button styling for dropdown toggle */
        #manageProgramsToggle, #manageCoursesToggle {
            cursor: pointer;
            transition: all 0.2s ease-in-out;
        }
        
        #manageProgramsToggle:hover, #manageCoursesToggle:hover {
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%) !important;
            transform: translateX(3px);
            border-color: #dee2e6;
        }
        
        #manageProgramsToggle:focus, #manageCoursesToggle:focus {
            outline: none;
            box-shadow: none;
        }
        
        /* Enhanced sidebar styling - integrated and cohesive */
        #sidebar {
            background: linear-gradient(180deg, #ffffff 0%, #f8f9fa 50%, #f1f3f4 100%);
            border-right: 1px solid #dee2e6;
            box-shadow: 2px 0 8px rgba(0,0,0,0.05);
        }
        
        /* Navigation header styling */
        #sidebar h5 {
            color: #495057;
            font-weight: 600;
            background: linear-gradient(135deg, #0d6efd 0%, #0b5ed7 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            border-bottom: 2px solid #0d6efd;
            padding-bottom: 0.5rem;
            margin-bottom: 1rem;
        }
        
        /* Icon styling with colors */
        #sidebar .nav-link i {
            width: 20px;
            text-align: center;
            transition: all 0.2s ease-in-out;
        }
        
        /* Dashboard icon - Blue */
        #sidebar .nav-link i.fa-home {
            color: #0d6efd;
        }
        
        /* Manage Programs icon - Purple */
        #sidebar .nav-link i.fa-cogs {
            color: #6f42c1;
        }
        
        /* Manage Tutors icon - Green */
        #sidebar .nav-link i.fa-user-tie {
            color: #198754;
        }
        
        /* Manage Students icon - Orange */
        #sidebar .nav-link i.fa-graduation-cap {
            color: #fd7e14;
        }
        
        /* Manage Courses icon - Teal */
        #sidebar .nav-link i.fa-book {
            color: #20c997;
        }
        
        /* Manage Assessments icon - Red */
        #sidebar .nav-link i.fa-file-alt {
            color: #dc3545;
        }
        
        /* LO Scores icon - Indigo */
        #sidebar .nav-link i.fa-list {
            color: #6610f2;
        }
        
        /* LO Results icon - Pink */
        #sidebar .nav-link i.fa-chart-line {
            color: #d63384;
        }
        
        /* Submenu icons - Lighter versions */
        #sidebar .nav-sub-item i.fa-calendar-week {
            color: #a78bfa;
        }
        
        #sidebar .nav-sub-item i.fa-certificate {
            color: #a78bfa;
        }
        
        #sidebar .nav-sub-item i.fa-list {
            color: #5dd9c1;
        }
        
        #sidebar .nav-sub-item i.fa-chalkboard-teacher {
            color: #5dd9c1;
        }
        
        /* Hover effects for icons */
        #sidebar .nav-link:hover i {
            transform: scale(1.1);
            filter: brightness(1.2);
        }
        
        /* Active state - icons become white */
        #sidebar .nav-link.active i {
            color: #ffffff !important;
            transform: scale(1.05);
        }
        
        /* Compact submenu styling */
        #manageProgramsCollapse .nav-link,
        #manageCoursesCollapse .nav-link {
            padding-top: 0.25rem;
            padding-bottom: 0.25rem;
            margin-bottom: 0.25rem;
            border-radius: 0.375rem;
        }
        
        /* Unified hover effects for all nav items */
        #sidebar .nav-link {
            border-radius: 0.5rem;
            transition: all 0.2s ease-in-out;
        }
        
        /* Smooth transitions for cohesive feel */
        #sidebar * {
            transition: all 0.2s ease-in-out;
        }
        
        /* Enhanced icon effects */
        #sidebar .nav-link i {
            text-shadow: 0 1px 2px rgba(0,0,0,0.1);
        }
        
        /* Icon pulse animation on hover */
        #sidebar .nav-link:hover i {
            animation: iconPulse 0.6s ease-in-out;
        }
        
        @@keyframes iconPulse {
            0% { transform: scale(1); }
            50% { transform: scale(1.15); }
            100% { transform: scale(1.1); }
        }
        
        /* Active icon glow effect */
        #sidebar .nav-link.active i {
            text-shadow: 0 0 8px rgba(255,255,255,0.8);
            animation: iconGlow 2s ease-in-out infinite alternate;
        }
        
        @@keyframes iconGlow {
            from { text-shadow: 0 0 8px rgba(255,255,255,0.8); }
            to { text-shadow: 0 0 12px rgba(255,255,255,1); }
        }
        
         /* Mobile sidebar */
         @@media (max-width: 991.98px) {
             #sidebar {
                 position: fixed;
                 top: 80px;
                 left: -280px;
                 z-index: 1030;
                 height: calc(100vh - 80px);
                 transform: translateX(0);
             }
            
            #sidebar.show {
                transform: translateX(280px);
            }
            
            #sidebarToggle {
                display: block !important;
            }
        }
        
        @@media (min-width: 992px) {
            #sidebarToggle {
                display: none !important;
            }
        }
        
          /* Header styles */
         .navbar-brand {
             font-size: 1.25rem;
         }
         
         /* Ensure header content is vertically centered */
         header .navbar {
             padding-top: 0;
             padding-bottom: 0;
         }
         
         /* Prevent header from shifting */
         header {
             position: relative;
             z-index: 1031;
         }
         
         /* Ensure header stays fixed at top */
         header .container-fluid {
             position: relative;
         }
        
         /* Content area */
         main {
             min-height: calc(100vh - 80px);
         }
        
        /* Button hover effects */
        .btn-outline-light:hover {
            background-color: rgba(255, 255, 255, 0.1);
            border-color: rgba(255, 255, 255, 0.5);
        }
        
        /* Smooth transitions for specific elements only */
        .btn, .nav-link, .form-control, .card {
            transition: all 0.2s ease-in-out;
        }
        
        /* Prevent header from shifting when modal opens */
        header {
            transition: none !important;
        }
        
        /* Ensure body doesn't shift when modal opens */
        body.modal-open {
            padding-right: 0 !important;
        }
    </style>
    
    <!-- Bootstrap Datepicker JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
